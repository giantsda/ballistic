timeStep=10000;
V0=2800;
Cd=0.002;
pi=3.14159265358;
theta=2*pi/180;
Vx0=V0*cos(theta);
Vy0=V0*sin(theta);
t=0;
tEnd=800/V0;
x=0;
y=0;
timeStep=5000;
dt=tEnd/timeStep;

g=32.1740;% ft/s2
Vxs=zeros(timeStep,1);
Vys=zeros(timeStep,1);
Position=zeros(timeStep,2);

Vx=Vx0;
Vy=Vy0;

%% Eluer
for i=1:timeStep
    Vx=Vx-Cd*Vx*Vx*dt;
    Vy=Vy-Cd*Vx*Vy*dt-g*dt;
    x=x+Vx*dt;
    y=y+Vy*dt;
    Vxs(i)=Vx;
    Vys(i)=Vy;
    Position(i,:)=[x y];
end
plot(Position(:,1),Position(:,2));



